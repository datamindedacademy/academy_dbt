{
    "name": "Academy dbt codespace",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "containerEnv": {
        "SHELL": "/bin/bash"
    },
    "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
    "customizations": {
        "vscode": {
            "extensions": [
                "innoverio.vscode-dbt-power-user@0.51.2",
                "ms-python.python@2024.22.2",
                "ms-python.debugpy",
                "samuelcolvin.jinjahtml"
            ]
        }
    },
    "remoteUser": "gitpod",
    "features": {}
}